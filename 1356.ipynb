   "source": [
    "N = input()\n",
    "ok = 0\n",
    "for i in range(1, len(N)):\n",
    "    s1, s2 = N[:i], N[i:]\n",
    "    m1 = m2 = 1\n",
    "    for n in s1:\n",
    "        m1 *= int(n)\n",
    "    for n in s2:\n",
    "        m2 *= int(n)\n",
    "    if m1 == m2:\n",
    "        ok = 1\n",
    "        break\n",
    "print(\"YES\" if ok else \"NO\")"
   ]
  }
