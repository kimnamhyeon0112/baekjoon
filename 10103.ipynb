    "round = int(input())\n",
    "A = B = 100\n",
    "for i in range(round):\n",
    "    a,b = map(int, input().split())\n",
    "    if a > b:\n",
    "        B -= a\n",
    "    elif a < b:\n",
    "        A -= b\n",
    "    else:\n",
    "        continue\n",
    "print(A)\n",
    "print(B)"
