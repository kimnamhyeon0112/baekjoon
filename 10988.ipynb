    "    make_list.append(i)\n",
    "\n",
    "while left < right:\n",
    "    if word[left] != word[right]:\n",
    "        print(0)\n",
    "        break\n",
    "    left += 1\n",
    "    right -= 1\n",
    "else:\n",
    "    print(1)"
