   "source": [
    "# 사람이 하는 방식으로 나누기 할 것\n",
    "# 단순 나누기 연산 //을 사용 할 경우 15자리까지만 정확히 연산하므로 정확한 값 산출 불가\n",
    "A,B = map(int, input().split())\n",
    "# 몫 출력\n",
    "print(A//B, end=\".\")\n",
    "\n",
    "# 나머지는 이어서 붙임\n",
    "if A % B:\n",
    "    for i in range(1000):\n",
    "        A %= B\n",
    "        A *= 10\n",
    "        print(A//B, end=\"\")"
   ]
  }
