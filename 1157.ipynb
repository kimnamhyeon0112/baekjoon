   "source": [
    "sentence = input().lower()\n",
    "word_list = list(set(sentence))\n",
    "make_list = []\n",
    "for i in word_list:\n",
    "    cnt = sentence.count(i)\n",
    "    make_list.append(cnt)\n",
    "\n",
    "if make_list.count(max(make_list)) >= 2:\n",
    "    print(\"?\")\n",
    "else:\n",
    "    print(word_list[(make_list.index(max(make_list)))].upper())"
   ]
  }
